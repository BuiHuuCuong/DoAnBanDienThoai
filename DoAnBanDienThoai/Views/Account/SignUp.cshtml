@{
    ViewData["Title"] = "SignUp";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<!--login and register section start here-->
<div class="mg-login-reg-section">
    <div class="container">
        <div class="row">
            <div style="margin-left: 25%;" class="col-lg-6 col-md-6 col-sm-12">
                <div class="mg-account-details-form">
                    <div class="mg-register-form-heading">
                        <span>Register</span>
           
                    </div>
                    <form>
                        <div class="mg-form-main">
                            <div class="row">
                                <div class="col-lg-12 col-md-12 col-sm-12">
                                    <div class="mg-form-input-field">
                                        <input type="text" placeholder="Username">
                                    </div>
                                </div>
                                <div class="col-lg-12 col-md-12 col-sm-12">
                                    <div class="mg-form-input-field">
                                        <input type="text" placeholder="Email">
                                    </div>
                                </div>
                                <div class="col-lg-12 col-md-12 col-sm-12">
                                    <div class="mg-form-input-field">
                                        <input type="text" placeholder="Password">
                                    </div>
                                </div>
                                <div>
                                  
                                </div>
                                <div class="mg-form-submit-btn">
                                    <button class="btn">REGISTER</button>
                                </div>
                            </div>
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </div>
</div>
<!--login and register section end here-->


